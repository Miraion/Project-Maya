section text


global main:
    la      src, r0
    la      dst, r1
    set.b   4, r2
    call    copy_memory

    load.l  src, r0
    call    putu
    call    putln

    load.l  dst, r0
    call    putu
    call    putln

    zero    rx
    ret

section data

local dst:  
    long 0x0

local src:  
    long 0x12345678
